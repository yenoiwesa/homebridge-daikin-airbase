{
    "name": "homebridge-daikin-airbase",
    "version": "4.0.1",
    "description": "A Homebridge plugin providing support for the Daikin Airbase.",
    "main": "dist/index.js",
    "files": [
        "/index.js",
        "/dist",
        "/config.schema.json"
    ],
    "scripts": {
        "build": "rimraf ./dist && tsc",
        "prepublishOnly": "npm run build",
        "start": "homebridge -D -U ./homebridge-home -P .",
        "debug": "ndb npm start",
        "lint:prettier": "prettier '**/*.{js,ts,json,md,yml}' --write",
        "lint:eslint": "eslint 'src/**/*.ts' --fix",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "watch": "npm run build && npm link && nodemon"
    },
    "keywords": [
        "homebridge-plugin",
        "homebridge",
        "daikin",
        "airbase",
        "skyfi",
        "BRP15B61"
    ],
    "author": "Matthieu Di Berardino",
    "license": "Apache-2.0",
    "devDependencies": {
        "@types/debounce-promise": "^3.1.9",
        "@types/lodash": "^4.17.20",
        "@types/node": "^24.10.0",
        "@types/node-fetch": "^2.6.13",
        "@types/retry": "^0.12.5",
        "@typescript-eslint/eslint-plugin": "^8.46.3",
        "@typescript-eslint/parser": "^8.46.3",
        "eslint": "^8.3.0",
        "homebridge": "^1.3.8",
        "prettier": "^2.5.0",
        "rimraf": "^6.1.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=20.0.0",
        "homebridge": ">=1.0.0 || >=2.0.0-beta.0"
    },
    "dependencies": {
        "debounce-promise": "^3.1.2",
        "lodash": "^4.17.21",
        "node-fetch": "^2.6.7",
        "retry": "^0.13.1"
    },
    "homepage": "https://github.com/yenoiwesa/homebridge-daikin-airbase#readme",
    "repository": {
        "type": "git",
        "url": "git://github.com/yenoiwesa/homebridge-daikin-airbase.git"
    },
    "bugs": {
        "url": "http://github.com/yenoiwesa/homebridge-daikin-airbase/issues"
    }
}
